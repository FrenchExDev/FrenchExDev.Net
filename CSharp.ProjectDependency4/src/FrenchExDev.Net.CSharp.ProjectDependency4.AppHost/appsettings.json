{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "AllowAnonymous": true,
  "AppsConfiguration": {
    "Apps": {
      "Viz": {
        "Kind": "WebUi",
        "Instances": 2,
        "Zeroes": 2,
        "Domain": "Viz"
      },
      "Viz.Api": {
        "Kind": "WebApi",
        "Instances": 1,
        "Domain": "Api",
        "Zeroes": 2
      },
      "Viz.Orchestrator": {
        "Kind": "WebWorker",
        "Instances": 1,
        "Domain": "Orchestrator",
        "Zeroes": 2
      },
      "Viz.Worker": {
        "Kind": "WebWorker",
        "Instances": 1,
        "Domain": "Worker",
        "Zeroes": 2
      }
    }
  },
  "DnsConfiguration": {
    "Subdomains": {
      "Dashboard": {
        "Domain": "dashboard",
        "Port": 4000
      },
      "Viz": {
        "DomainTemplate": "viz-$i",
        "Ipv4": "127.0.0.1",
        "PortRangeStart": 5000
      },
      "Orchestrator": {
        "DomainTemplate": "orchestrator-$i",
        "Ipv4": "127.0.0.1",
        "PortRangeStart": 6000
      },
      "Api": {
        "DomainTemplate": "api-$i",
        "Ipv4": "127.0.0.1",
        "PortRangeStart": 7000
      },
      "Worker": {
        "DomainTemplate": "worker-$i",
        "Ipv4": "127.0.0.1",
        "PortRangeStart": 8000
      }
    },
    "CertificatesDirectory": ".aspire-certs"
  }
}
