<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Version>0.0.1</Version>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
		<Description>Tiny Modular Monolith framework in .NET</Description>
		<Copyright>FrenchExDev</Copyright>
		<PackageProjectUrl></PackageProjectUrl>
		<RepositoryUrl>https://www.github.com/FrenchExDev.Net/FrenchExDev.Net</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>modular monolith pattern</PackageTags>
		<IncludeSymbols>True</IncludeSymbols>
		<EnforceCodeStyleInBuild>True</EnforceCodeStyleInBuild>
	</PropertyGroup>

	<ItemGroup Label="Deps.FrenchExDev">
	  <PackageReference Include="FrenchExDev.Net.CSharp.Object.Builder" />
	</ItemGroup>

	
</Project>
