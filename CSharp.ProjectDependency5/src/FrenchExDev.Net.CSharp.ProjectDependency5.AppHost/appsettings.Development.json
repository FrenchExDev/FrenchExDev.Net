{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowAnonymous": true,
  "AppsConfiguration": {
    "Apps": {
      "DevDashboard": {
        "Kind": "WebUi",
        "Instances": 1,
        "Domain": "DevDashboard",
        "IsAspireResource": true
      },
      "Viz": {
        "Kind": "WebUi",
        "Instances": 1,
        "Zeroes": 2,
        "Domain": "Viz",
        "Enabled": true
      },
      "Viz.Api": {
        "Kind": "WebApi",
        "Instances": 1,
        "Domain": "Viz.Api",
        "Zeroes": 2,
        "Enabled":  false
      },
      "Viz.Worker": {
        "Kind": "WebWorker",
        "Instances": 1,
        "Domain": "Viz.Worker",
        "Zeroes": 2,
        "Enabled": false
      }
    }
  },
  "DnsConfiguration": {
    "Domain": "pd5i1.com",
    "Subdomains": {
      "DevDashboard": {
        "Domain": "devdash",
        "Port": 4000,
        "Ipv4": "127.0.0.1"
      },
      "Viz": {
        "DomainTemplate": "viz-$i",
        "Ipv4": "127.0.0.1",
        "PortRangeStart": 5000
      },
      "Viz.Api": {
        "DomainTemplate": "viz-api-$i",
        "Ipv4": "127.0.0.1",
        "PortRangeStart": 7000
      },
      "Viz.Worker": {
        "DomainTemplate": "viz-worker-$i",
        "Ipv4": "127.0.0.1",
        "PortRangeStart": 8000
      }
    },
    "CertificatesDirectory": ".aspire-certs"
  }
}
